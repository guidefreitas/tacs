@model Tacs.ViewModels.Teste.VMCreateEditTeste

<div class="table-responsive">
    <table class="table table-striped table_tacs">
        <thead>
            <tr>
                <td>Id</td>
                <td>TesteId</td>
                <td>QuestaoId</td>
                <td>Questao</td>
                <td>Usuario</td>
                <td>AlternativaId</td>
            </tr>
        </thead>
        <tbody>
            @if (Model.Itens.Count == 0)
            {
                <tr>
                    <td>Nenhuma resposta cadastrada</td>
                    <td></td>
                    <td></td>
                    <td></td>
                    <td></td>
                    <td></td>
                </tr>
            }
            else
            {
                foreach (var q in Model.Itens)
                {
                    <tr>
                        <td>@q.Id</td>
                        <td>@q.QuestaoId</td>
                        <td>@q.Questao</td>
                        <td>@q.UsuarioNome</td>
                        <td>@q.AlternativaId</td>
                    </tr>
                }
            }
        </tbody>
    </table>
</div>

